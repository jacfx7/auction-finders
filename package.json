{
  "name": "auction-finders",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/styles": "^4.4.3",
    "bootstrap": "^4.3.1",
    "firebase": "^6.6.2",
    "jquery": "^3.4.1",
    "material-ui": "^0.20.2",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "ra-core": "^2.9.6",
    "ra-input-rich-text": "^2.9.3",
    "react": "^16.9.0",
    "react-admin": "^2.9.6",
    "react-admin-firebase": "^0.8.9",
    "react-dom": "^16.9.0",
    "react-google-button": "^0.5.3",
    "react-redux": "~5.0.7",
    "react-router": "^5.1.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "source-map-loader": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/tools/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/tools/styleMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/tools/*.js",
      "!src/config/*.js",
      "!src/store/*.js",
      "!src/constants/*.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0"
  }
}
